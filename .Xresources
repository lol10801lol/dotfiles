rofi.width: 1920
rofi.lines: 1
rofi.fixed-num-lines: true
rofi.columns: 10
rofi.location: 1
rofi.separator-style: none
rofi.hide-scrollbar: true
rofi.case-sensitive: false
rofi.levenshtein-sort: false
rofi.auto-select: true
rofi.font: Ubuntu 14
rofi.color-enabled: true
rofi.color-window: argb:9c2d2d2d, #002b36, #000000
rofi.color-normal: argb:02d2d2d, #a2a2a2, #eee8d5, #2d2d2d, #a2a2a2
rofi.color-active: #2d2d2d, #a2a2a2, #eee8d5, #268bd2, #fdf6e3
rofi.color-urgent: #fdf6e3, #dc322f, #eee8d5, #dc322f, #fdf6e3
! special
*.foreground:   #d0d0d0
*.background:   #151515
*.cursorColor:  #d0d0d0

! black
*.color0:       #151515
*.color8:       #505050

! red
*.color1:       #ac4142
*.color9:       #ac4142

! green
*.color2:       #90a959
*.color10:      #90a959

! yellow
*.color3:       #f4bf75
*.color11:      #f4bf75

! blue
*.color4:       #6a9fb5
*.color12:      #6a9fb5

! magenta
*.color5:       #aa759f
*.color13:      #aa759f

! cyan
*.color6:       #75b5aa
*.color14:      #75b5aa

! white
*.color7:       #d0d0d0
*.color15:      #f5f5f5

